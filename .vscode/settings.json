{
  "css.customData": ["./vscode.tailwind.json"],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro", // Enable .astro
    "typescript", // Enable .ts
    "typescriptreact" // Enable .tsx
  ],
  "files.associations": {
    "*.mdx": "markdown"
  },
  "prettier.documentSelectors": ["**/*.astro"],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "addjson",
    "astro",
    "astrojs",
    "cantomaze",
    "cantotest",
    "Cantoverse",
    "collabs",
    "connectkit",
    "howto",
    "mdast",
    "Metaverse",
    "Navi",
    "partytown",
    "presale",
    "tabler",
    "Testv",
    "Totalmints",
    "Wagmi",
    "webp"
  ]
}
